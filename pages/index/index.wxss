.header {
  padding: 10rpx 40rpx;
  display: flex;
  justify-content: space-between;
}
.header-avatar {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  overflow: hidden;
}
.header-left {
  transform: translateX(-45rpx);
  display: flex;
  align-items: center;
}
.header-right {
  display: flex;
  color: #000;
}
.header-right view {
  display: flex;
  align-items: center;
}
.header-right .iconfont {
  font-size: 32rpx;
  margin-right: 10rpx;
}
.search-word {
  padding-right: 40rpx;
}

.progress {
  padding: 40rpx 50rpx;
  margin: 20rpx 50rpx;
  border-radius: 40rpx;
  background-color: #f7f6f1;
}
.progress-plan {
  padding: 20rpx 70rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.font-color {
  color: #85817c;
  display: block;
  margin-bottom: -60rpx;
}
.big {
  font-size: 64rpx;
}
.small {
  font-size: 32rpx;
}
.text {
  text-indent: 15rpx;
}

.progress-all {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #85817c;
  padding-top: 40rpx;
  padding-bottom: 30rpx;
  border-bottom: 3rpx solid #e6e4df;
}
.book {
  color: #42aff4;
  padding-top: 10rpx;
}
.progress-all__right {
  border: 2rpx solid #e6e4df;
  border-radius: 30rpx;
  padding: 10rpx 20rpx;
}
.progress-complete {
  font-size: 26rpx;
  color: #85817c;
  padding-top: 50rpx;
}
.progress-complete__top,
.icons {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.icons {
  font-size: 20rpx;
  text-align: center;
}
.icons > view {
  padding-left: 10rpx;
}
.progress-complete__bottom {
  border: 2rpx solid #70c0ec;
  border-radius: 10rpx;
  padding: 4rpx;
  margin-top: 20rpx;
}
.inner {
  background-color: #5dbcf6;
  width: 80%;
  height: 10rpx;
  border-radius: 10rpx;
}

.footer {
  margin: 50rpx 50rpx;
  text-align: center;
  color: #85817c;
}
.start {
  height: 110rpx;
  background-color: #55a8de;
  color: #fff;
  line-height: 110rpx;
  margin-bottom: 30rpx;
  border-radius: 15rpx;
}
.read {
  height: 100rpx;
  line-height: 100rpx;
  color: #55a8de;
  border-radius: 15rpx;
}